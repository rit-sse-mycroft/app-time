<?xml version="1.0"?>
<grammar xmlns="http://www.w3.org/2001/06/grammar" xml:lang="en-US" root="time" version="1.0" tag-format="semantics/1.0">
  <rule id="time" scope="public">
    <item>Mycroft</item>
    <one-of>
      <item>do you have the time</item>
      <item>what time is it</item>
      <item>what day is it</item>
      <item>
        <one-of>
          <item>what's</item>
          <item>what is</item>
        </one-of>
        <one-of>
          <item>the day</item>
          <item>the date</item>
          <item>the time</item>
          <item>today</item>
        </one-of>
      </item>
    </one-of>
  </rule>
</grammar>
